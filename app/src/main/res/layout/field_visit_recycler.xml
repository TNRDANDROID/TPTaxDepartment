        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/new_round_cornered_bg"
            android:id="@+id/rel1">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_centerVertical="true"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:id="@+id/nameValue"
                    android:text="Owner Name"
                    android:fontFamily="sans-serif" />
                <ImageView
                    android:padding="@dimen/_5sdp"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:id="@+id/image_list"
                    android:tint="@color/white"
                    android:layout_alignParentLeft="true"
                    android:layout_gravity="end"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/ic_gallery"/>
                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:id="@+id/delete"
                    android:tint="@color/white"
                    android:padding="8dp"
                    android:layout_alignParentRight="true"
                    android:layout_gravity="end"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/ic_trash"/>
            </RelativeLayout>



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/rel1"
            android:weightSum="3"
            android:layout_marginTop="5dp"
            android:padding="10dp"
            android:id="@+id/rel2">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:id="@+id/name"
                android:text="@string/request_id"
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:layout_toRightOf="@+id/name"
                android:layout_below="@+id/rel1"
                android:id="@+id/namec"
                android:text=" : "
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.5"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textColor="@color/border_line"
                android:textSize="16sp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/namec"
                android:layout_below="@+id/rel1"
                android:id="@+id/codeValue"
                android:text="Request ID"
                android:fontFamily="sans-serif" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/rel2"
            android:weightSum="3"
            android:paddingTop="0dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:id="@+id/rel3">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:id="@+id/mobile"
                android:layout_below="@+id/name"
                android:text="@string/tax_type"
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:layout_toRightOf="@+id/name"
                android:layout_below="@+id/name"
                android:id="@+id/mobilec"
                android:text=" : "
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.9"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textColor="@color/border_line"
                android:textSize="16sp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/mobilec"
                android:layout_below="@+id/name"
                android:id="@+id/taxTypeValue"
                android:text="Mobile No"
                android:fontFamily="sans-serif" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/rel3"
            android:weightSum="3"
            android:paddingTop="3dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:id="@+id/rel4">
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:id="@+id/paymentDate"
                android:layout_below="@+id/mobile"
                android:text="@string/current_status"
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="0.1"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:textColor="@color/grey1"
                android:layout_toRightOf="@+id/name"
                android:layout_below="@+id/mobile"
                android:id="@+id/paymentdatec"
                android:text=" : "
                android:fontFamily="sans-serif" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1.9"
                android:layout_height="match_parent"
                android:gravity="start"
                android:textSize="16sp"
                android:layout_marginLeft="5dp"
                android:textColor="@color/border_line"
                android:layout_toRightOf="@+id/paymentdatec"
                android:layout_below="@+id/mobile"
                android:id="@+id/status_filed"
                android:text="Status"
                android:fontFamily="sans-serif" />
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </RelativeLayout>
</RelativeLayout>



