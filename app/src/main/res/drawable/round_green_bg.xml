<?xml version="1.0" encoding="UTF-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/account_status_green_color">
    <item>
        <shape >
            <solid android:color="@color/account_status_green_color" />
            <corners
                android:topRightRadius="0dp"
                android:topLeftRadius="10dp"
                android:bottomRightRadius="10dp"
                android:bottomLeftRadius="0dp"
                />
            <stroke
                android:width="1dip"
                android:color="@color/account_status_green_color" />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip" />
        </shape>
    </item>
</ripple>

