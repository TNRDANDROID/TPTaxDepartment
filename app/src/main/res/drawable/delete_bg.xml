<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="-50dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            android:useLevel="false">
            <size
                android:width="80dp"
                android:height="80dp" />
            <gradient
                android:angle="270"
                android:startColor="@color/subscription_type_red_color"
                android:endColor="@color/subscription_type_red_color"/>
            <corners android:bottomLeftRadius="6dp" />
            <stroke
                android:width="1dip"
                android:color="@color/subscription_type_red_color" />

        </shape>
    </item>
</layer-list>